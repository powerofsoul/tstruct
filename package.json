{
  "name": "tstruct",
  "version": "0.0.8",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "rm -rf ./dist && cti ./src --fileexcludes *.spec.ts && npm run build && npm run build:types ",
    "build:types": "tsc -d",
    "build:docs": "rm -rf docs && typedoc --plugin typedoc-plugin-markdown --exclude **/*.spec.ts --exclude **/index.ts",
    "build": "npm run lint && tsc",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint ./src/ --ext .ts --fix --quiet"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/powerofsoul/tstruct.git"
  },
  "author": "Florin Munteanu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/powerofsoul/tstruct/issues"
  },
  "homepage": "https://github.com/powerofsoul/tstruct#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "create-index": "^2.6.0",
    "create-ts-index": "^1.13.3",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "ts-jest": "^26.2.0",
    "typedoc": "^0.18.0",
    "typedoc-plugin-markdown": "^2.4.2",
    "typescript": "^3.9.7"
  },
  "type": "tstruct"
}
